{
  "name": "membercore",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "setup:dependencies": "bash scripts/install_dependencies.sh",
    "setup:vertragsarten-v2": "bash scripts/setup_vertragsarten_v2.sh",
    "setup:supabase": "bash scripts/setup_supabase_v2.sh",
    "db:migrate": "supabase db push",
    "db:reset": "supabase db reset",
    "db:seed": "supabase db exec --file supabase/migrations/20241201_vertragsarten_v2_data.sql",
    "db:status": "supabase status",
    "v2:install": "npm run setup:dependencies && npm run setup:supabase && echo '🎉 Vertragsarten V2 System erfolgreich installiert!'",
    "v2:dev": "npm run db:status && npm run dev",
    "v2:test": "supabase db exec \"SELECT COUNT(*) FROM contracts; SELECT COUNT(*) FROM module_categories; SELECT COUNT(*) FROM contract_modules;\""
  },
  "dependencies": {
    "@react-pdf/renderer": "^4.3.0",
    "@supabase/auth-helpers-nextjs": "^0.8.7",
    "@supabase/supabase-js": "^2.38.4",
    "@types/pdf-parse": "^1.1.5",
    "better-sepa": "^0.0.5",
    "buffer": "^6.0.3",
    "dotenv": "^16.5.0",
    "lucide-react": "^0.279.0",
    "next": "^13.5.4",
    "pdf-parse": "^1.1.1",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dnd-touch-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "recharts": "^2.8.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/react": "^18.2.24",
    "@types/react-dom": "^18.2.8",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.50.0",
    "eslint-config-next": "^13.5.4",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2"
  }
}
